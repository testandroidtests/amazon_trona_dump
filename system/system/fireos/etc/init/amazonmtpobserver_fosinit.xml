<fosinit xmlns="http://www.amazon.com"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.amazon.com/fireos/base/core/config/ fosinit.xsd">
    <vendor-services>
        <service impl="com.amazon.android.service.mtp.AmazonMtpObserverService" />
    </vendor-services>
    <vendor-callbacks>
        <callback base="com.android.server.usb.VendorUsbDeviceManagerCallback"
            impl="com.amazon.android.service.mtp.AmazonUsbDeviceManagerCallback"
            classLoader="SYSTEMSERVER" />
    </vendor-callbacks>
    <vendor-managers>
        <manager name="AmazonMtpObserverService"
            base="com.amazon.android.service.mtp.AmazonMtpObserverManager"
            fetcher="STATIC" />
    </vendor-managers>
</fosinit>
