{
  "acoustic-model": {
    "non-speech-pdf": -2,
    "speech-pdf": -1
  },
  "keyword-orchestra": {
    "buffer-size": 200,
    "keyword-spotters": [
      {
        "classifier": {
          "type": "vector-space",
          "vector-space": {
            "vector-space-classifier": {
              "map": [
                {
                  "classifier": {
                    "normalized-vector-space-classifier": {
                      "classifier": {
                        "svm": {
                          "filename": "AMAZON.psvm",
                          "probabilistic": false
                        },
                        "type": "svm"
                      },
                      "normalizer": {
                        "scale": {
                          "filename": "AMAZON.scales"
                        },
                        "type": "scale"
                      }
                    },
                    "type": "normalized-vector-space-classifier"
                  },
                  "name": "AMAZON"
                }
              ],
              "type": "map"
            },
            "vector-space-mapper": {
              "type": "version1",
              "version1": {}
            }
          }
        },
        "hmm": {
          "bg": {
            "filename": "AMAZON.bg.hclg.pfst",
            "type": "pryon-fst"
          },
          "fg": {
            "filename": "AMAZON.fg.hclg.pfst",
            "type": "pryon-fst"
          },
          "search-space": {
            "beam": 25.0,
            "block-size": 1024,
            "max-depth": 200
          },
          "window-size": 6
        },
        "name": "AMAZON",
        "spotter": {
          "classification-limits": {
            "max-per-window": 3,
            "stickiness": 0,
            "window-size": 100
          },
          "classification-thresholds": {
            "accept-threshold": -1.125,
            "escalate-threshold": 1e+37,
            "notify-threshold": -2.0
          },
          "cleanup-period": 6000,
          "escalation-period": 200,
          "hmm-thresholds": {
            "accept-threshold": 0.0,
            "escalate-threshold": 1e+37,
            "notify-threshold": 0.0
          },
          "lock-period": 40
        }
      }
    ]
  },
  "keyword-orchestra-monitor": {
    "statistics": {
      "classification": {
        "histogram": {
          "max": 4,
          "min": -4,
          "size": 50
        }
      },
      "segmentation": {
        "histogram": {
          "max": 40,
          "min": -30,
          "size": 50
        }
      }
    }
  },
  "object_type_name": "keyword-spotter-orchestra-config",
  "object_type_version": 2,
  "phone-symbol-table": {
    "filename": "phones.txt"
  },
  "word-symbol-table": {
    "filename": "words.shrunk.txt"
  }
}